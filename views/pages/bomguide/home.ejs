	<button id="debugToggleLogin" style="color: red">Debug Toggle Login</button>
	<span><%= loggedIn %><%= user %></span>

	<div id="loginDiv">
		<button id="buttonLoadLogin">Login |</button><button id="buttonLoadRegister" style="margin-left: 5px">Register</button><hr>
